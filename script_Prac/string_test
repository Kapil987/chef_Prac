# USERNAME = `whoami` <-- not recommended 
# execute this script once with root once without root
# sudo ./

#!/bin/bash

USERNAME=`/usr/bin/whoami`
if [ "$USERNAME" == "root" ]; then
echo "you are root user"
else 
echo "you are not root user"
fi

